{
    "name": "gearhub/laravel-enhancement-suite",
    "description": "Additions that are used in most of our Laravel projects.",
    "keywords": ["gear", "gearhub", "laravel"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vince Ruby",
            "email": "vsruby91@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.5",
        "illuminate/console": "^5.5",
        "league/fractal": "^0.16.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "files": [
          "src/helpers.php"
        ],
        "psr-4": {
            "GearHub\\LaravelEnhancementSuite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GearHub\\LaravelEnhancementSuite\\Tests\\": "tests/"
        }
    }
}
